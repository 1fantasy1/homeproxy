{
	"luci-app-homeproxy": {
		"description": "Grant access to homeproxy configuration",
		"read": {
			"file": {
				"/etc/homeproxy/*": [ "read" ],
				"/usr/bin/which": [ "exec" ],
				"/bin/mkdir -p /etc/homeproxy/certs/": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": [ "homeproxy" ]
		},
		"write": {
			"file": {
				"/etc/homeproxy/*": [ "write" ]
			},
			"uci": [ "homeproxy" ]
		}
	}
}
