{
	"luci-app-homeproxy": {
		"description": "Grant access to homeproxy configuration",
		"read": {
			"file": {
				"/etc/homeproxy/*": [ "read" ],
				"/etc/ssl/private/ca.pem": ["read"],
				"/bin/ash": [ "exec" ]
			},
			"ubus": {
				"service": [ "list" ]
			},
			"uci": [ "homeproxy" ]
		},
		"write": {
			"file": {
				"/etc/homeproxy/*": [ "write" ],
				"/etc/ssl/private/ca.pem": ["write"]
			},
			"uci": [ "homeproxy" ]
		}
	}
}
